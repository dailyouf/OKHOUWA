	.ORIG x3000
	
	
	AND R0 R0 #0
	
	AND R1 R1 #0
	ADD R1 R1 val_117
	
	ADD R1 R1 #11
	
	ADD R2 R1 #0
	
	AND R3 R2 x000F
	
	IN
	
	ST R0 y
	
	LD R4 y
	
	ST R4 z
	
	LEA R5 tab
	
	ST R5 p
	
	LD R6 p
	
	ADD R6 R6 #3
	
	STR R1 R6
	
	LD R6 y
	
	OUT
	
	HALT
	
	
val_117	.FILL #117
	
y	.blkw #1	
z	.blkw #1

p	.blkw #1
tab	.blkw #4


	.END
	
