#!/bin/sh


gcc generate_instances_inc.c -o generate_instances_inc

gcc solve_backtracking.c -o solve_backtracking

gcc solve_backtracking_optimised.c -o solve_backtracking_optimised


./generate_instances_inc 5 10 10 100 instances_inc.txt


./solve_backtracking instances_inc.txt results_backtracking_inc.txt

./solve_backtracking_optimised instances_inc.txt results_backtracking_optimised_inc.txt


python3 compare_algos_plot.py results_backtracking_inc.txt results_backtracking_optimised_inc.txt

